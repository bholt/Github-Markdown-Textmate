	<key>command</key>
	<string>#!/usr/bin/env ruby

line = $stdin.read

indent = "\t"
if ENV['TM_SOFT_TABS'] == 'YES'
  indent = ' ' * ENV['TM_TAB_SIZE'].to_i
end

if /^\t(\s*[-+*].*)/ =~ line
  $stdout.write($~[1])
else
  $stdout.write(line)
end
